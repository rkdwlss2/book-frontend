{
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        },
        "requireConfigFile": false,
        "babelOptions": {
          "parserOpts": {
            "plugins": ["jsx"]
          }
        }
      }
,    
"extends": ["eslint:recommended", "plugin:react/recommended"],

    "rules": {
        "react/prop-types":"off",
        "no-unused-vars":"off"
    }
  }
  